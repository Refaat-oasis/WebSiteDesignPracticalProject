 @model Doctor 

 <h1>Docotr Signup</h1> 
 <hr />
<form action="/Doctor/doctorSignupSave" method="post">
    <label>Name</label> 
     <input type="text" name="Name" id="Name" class="form-check form-control"> 
     <br /> 
     <label>Email</label> 
     <input type="text" name="Email" id="Email" class="form-check form-control"> 
     <br /> 
     <label>Password</label> 
     <input type="text" name="Password" id="Password" class="form-check form-control"> 
     <br /> 
     <label>Address</label> 
     <input type="text" name="Address" id="Address" class="form-check form-control"> 
     <br /> 
     <label>Age</label> 
     <input type="number" name="Age" id="Age" class="form-check form-control"> 
     <br /> 
     <label>PhoneNumber</label> 
     <input type="text" name="PhoneNumber" id="PhoneNumber" class="form-check form-control">
     <input type="submit" value="create new Doctor" class="btn btn-success" /> 

 </form> 

