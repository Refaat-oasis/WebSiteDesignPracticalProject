@model Admin 
 <!DOCTYPE html> 
 <html lang="en"> 
 <head> 
     <meta charset="UTF-8"> 
     <meta name="viewport" content="width=device-width, initial-scale=1.0"> 
     <title>Admin Dashboard</title> 
     <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet"> 
 </head> 
 <body class="container py-5"> 
     <div class="card shadow-sm"> 
         <div class="card-header bg-primary text-white"> 
             <h2>Welcome, @Model.Name</h2> 
         </div> 
         <div class="card-body"> 
             <p class="mb-4">Manage your profile and appointments below:</p> 
             <ul class="list-group"> 
                 <li class="list-group-item"> 
                     <a href="/Appointment/addAppointement" class="btn btn-link">Add New Appointment</a> 
                 </li> 
                 <li class="list-group-item"> 
                     <a href="/Appointment/showAdminAppointments" class="btn btn-link">Show All Appointments</a> 
                 </li> 
                 <li class="list-group-item"> 
                     <a href="/Admin/showMyProfile/@Model.AdminId" class="btn btn-link">My Profile</a> 
                 </li> 
                 <li class="list-group-item"> 
                     <a href="/Admin/adminLogout" class="btn btn-link text-danger">Log Out</a> 
                 </li> 
             </ul> 
         </div> 
     </div> 
 </body>
 </html> 
